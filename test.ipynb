   "source": [
    "def fibonacci(n):\n",
    "    if (n == 0 or n == 1):\n",
    "        return 1\n",
    "    return fibonacci(n - 1) + fibonacci(n - 2)\n",
    "\n",
    "\n",
    "print(\"Fibonacci of 10 is: \" + str(fibonacci(10)))"
